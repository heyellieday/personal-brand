 <section id="portfolio"  ng-controller="ModalIconCtrl">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2>What I do</h2>
                
                </div>
            </div>
            <div ng-repeat="row in rowsOfOrgs
            " class="row">
                <div ng-repeat="icon in row" class="col-sm-4 portfolio-item">
                    <a href="#portfolioModal{{icon.id}}" ng-click="showModal('{{icon.id}}')"  class="portfolio-link" data-toggle="modal">
                        <div class="caption">
                            <div class="caption-content">
                                <h3>{{icon.org}}</h3>
                                <h4>{{icon.title}}</h4>
                            </div>
                        </div>
                        <img src="{{icon.url}}" class="img-responsive" alt="">
                    </a>
                </div>
            </div>
            <div ng-repeat="row in rowsOfProjects
            " class="row">
                <div ng-repeat="icon in row" class="col-sm-3 portfolio-item">
                    <a href="#portfolioModal{{icon.id}}" ng-click="showModal('{{icon.id}}')"  class="portfolio-link" data-toggle="modal">
                        <div class="caption">
                            <div class="caption-content">
                                <h3>{{icon.org}}</h3>
                                <h4>{{icon.title}}</h4>
                            </div>
                        </div>
                        <img src="{{icon.url}}" class="img-responsive" alt="">
                    </a>
                </div>
            </div>
        </div>
</section>